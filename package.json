{
  "name": "ola",
  "version": "0.0.1",
  "main": "index.js",
  "author": "xigua",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "run-p \"watch icons\" \"watch components\" app:dev",
    "watch": "node scripts/dev.js",
    "app:dev": "pnpm --filter @ola/ola dev",
    "app:build": "pnpm --filter @ola/ola build",
    "storybook": "pnpm --filter @ola/components storybook",
    "components:build": "pnpm --filter @ola/components build",
    "icons:dev": "pnpm --filter @ola/icons dev",
    "all:build": "pnpm -r exec pnpm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "eslint:check": "eslint packages"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/react-dnd": "3.0.2",
    "@types/react-dnd-html5-backend": "3.0.2",
    "@typescript-eslint/parser": "^5.59.0",
    "esbuild": "^0.17.17",
    "esbuild-sass-plugin": "^2.9.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "minimist": "^1.2.8",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.7",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/react-dnd-multi-backend": "6.0.1",
    "@types/react-window": "1.8.5",
    "classnames": "2.3.1",
    "immutable": "^3.8.1",
    "memoize-one": "6.0.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dnd-touch-backend": "^0.2.5",
    "react-dnd-treeview": "^0.4.1",
    "react-dom": "^18.2.0",
    "react-feather": "2.0.10",
    "react-scripts": "4.0.3",
    "react-virtualized-auto-sizer": "1.0.6",
    "react-window": "1.8.7",
    "tree-model-improved": "2.0.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "bracketSpacing": true
  }
}
